{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "Movie",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "poster": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "watched": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "movieLength": {
      "default": 144,
      "required": true,
      "type": "integer"
    },
    "genres": {
      "type": "string"
    },
    "rating": {
      "type": "boolean",
      "required": false
    }
  }
}
