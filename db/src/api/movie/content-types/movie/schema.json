{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "Movie",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Poster": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Watched": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "movieLength": {
      "default": 144,
      "required": true,
      "type": "integer"
    },
    "genre": {
      "type": "enumeration",
      "enum": [
        "comedy",
        "horror",
        "family"
      ]
    }
  }
}
